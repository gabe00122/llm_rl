{
    "seed": "random",
    "base_model": "Qwen/Qwen3-4B-Instruct-2507",
    "eval_envs": 0,
    "update_envs": 32,
    "max_seq_length": 1024,
    "total_update_episodes": 16000,
    "checkpoint_every": 500,
    "lora": {
        "mlp": true,
        "attn": true,
        "rank": 32
    },
    "logger": {
        "use_console": true,
        "use_wandb": true
    },
    "value_optimizer": {
        "opt": {
            "type": "adamw",
            "lr": 0.0001,
            "beta1": 0.0,
            "beta2": 0.99,
            "weight_decay": 0.0001,
            "eps": 0.00001
        },
        "schedule": {
            "type": "warmup_cosine",
            "warmup_ratio": 0.1
        }
    },
    "loss": {
        "gae_lambda": 0.996,
        "gae_discount": 1.0,
        "vf_coef": 1.0,
        "pg_clip_high": 0.28,
        "pg_clip_low": 0.2
    },
    "env": {
        "name": "wordle",
        "max_guesses": 6
    },
    "value_net": {
        "laten_encoder_rank": 256,
        "backbone": {
            "embed": 256,
            "mlp_ffw_size": 512,
            "q_heads": 8,
            "kv_heads": 8,
            "num_layers": 12,
            "head_dim": 32
        },
        "head": {
            "type": "hl_gauss",
            "min": -0.1,
            "max": 1.1,
            "n_logits": 51,
            "sigma": 0.01
        }
    }
}
