  },
  "value_optimizer": {
    "opt": {
      "type": "adamw",
      "lr": 0.0002,
      "beta1": 0.9,
      "beta2": 0.999,
      "weight_decay": 0.0
    },
    "schedule": {
      "type": "warmup_cosine",
      "warmup_ratio": 0.1
    }
  },
  "loss": {
    "gae_lambda": 0.996,
    "gae_discount": 1.0,
    "vf_coef": 1.0,
    "pg_clip_high": 0.28,
    "pg_clip_low": 0.2
