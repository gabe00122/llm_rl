{
    "seed": "random",
    "base_model": "Qwen/Qwen3-4B-Instruct-2507",
    "eval_envs": 32,
    "update_envs": 4,
    "max_seq_length": 1024,
    "total_update_episodes": 40000,
    "checkpoint_every": 1000,
    "lora": {
        "mlp": true,
        "attn": true,
        "rank": 32
    },
    "logger": {
        "use_console": true,
        "use_wandb": true
    },
    "schedule": {
        "type": "warmup_cosine",
        "warmup_ratio": 0.1
    },
    "optimizer": {
        "type": "adamw",
        "lr": 0.00008,
        "beta1": 0.9,
        "beta2": 0.999,
        "weight_decay": 0.0
    },
    "loss": {
        "gae_lambda": 0.996,
        "gae_discount": 1.0,
        "vf_coef": 1.0,
        "pg_clip_high": 0.28,
        "pg_clip_low": 0.2
    },
    "env": {
        "name": "wordle",
        "max_guesses": 6
    }
}
